#include <bits/stdc++.h>

using namespace std;
vector <int> V(100000, 10);
long long A;
void f1(){
    for(auto i = 0; i < 100000; ++i)
            A+=V[i];
            this_thread::sleep_for(chrono::milliseconds(1000));
}
void f2(){
    for(auto i = 0; i < 100000; ++i)
            cout << A << " ";
            this_thread::sleep_for(chrono::milliseconds(1000));
}
int main(){
    vector <int> V(100000, 10);
    long long A = 0;
    thread t1(f1);
    t1.detach();
    thread t2(f2);
    t2.join();
}
